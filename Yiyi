<body>
    <h1>Acceso a Roblox</h1>
    <p>Usa este proxy para acceder a Roblox sin restricciones.</p>
    <iframe id="proxyFrame" src="https://www.roblox.com" sandbox="allow-scripts allow-same-origin allow-popups allow-forms"></iframe>

    <script>
        // Función para detectar si el iframe ha cargado correctamente
        function checkIframeLoad() {
            const iframe = document.getElementById('proxyFrame');
            iframe.onload = function() {
                alert('Roblox ha cargado exitosamente!');
            };
        }

        // Llamar a la función para verificar el cargado del iframe
        checkIframeLoad();
    </script>
